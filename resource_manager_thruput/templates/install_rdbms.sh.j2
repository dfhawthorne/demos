#!/usr/bin/bash
# ------------------------------------------------------------------------------
# Installs Oracle RDBMS 19.3 Under OL 8
# Implements recommendation from MOS Doc 2668780.1
# ------------------------------------------------------------------------------

export CV_ASSUME_DISTID=OL7

{{ oracle_home }}/runInstaller     \
  -silent                               \
  -noconfig                             \
  -responseFile \"{{ response_file }}\" \
  -waitforcompletion                    \
  -ignorePrereq
